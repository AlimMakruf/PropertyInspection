<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Overall_Rating_Range</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK( Overall_Rating__c )),
    OR(
        Overall_Rating__c &lt; 1,
        Overall_Rating__c &gt; 5
    )
)</errorConditionFormula>
    <errorDisplayField>Overall_Rating__c</errorDisplayField>
    <errorMessage>Overall rating will only range 1 - 5</errorMessage>
</ValidationRule>
